{
  "manifest_version": 3,
  "name": "Mango",
  "version": "0.1.1",
  "description": "Recomendation System for Codeforces Problems",
  "permissions": [],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "action": {
    "default_popup": "src/popup.html"
  }, "content_scripts": [
    {
      "matches": ["https://codeforces.com/problemset/problem/*", "https://codeforces.com/contest/*"],
      "js": ["src/content.js"]
    }
  ]
}