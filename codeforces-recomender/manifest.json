{
    "manifest_version": 3,
    "name": "Codeforces Recomender",
    "version": "0.1",
    "description": "Recomends problems",
    "permissions": ["activeTab", "storage", "unlimitedStorage"],
    "icons": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://codeforces.com/problemset/problem/*"],
        "js": ["content.js", "slider.js"]
      }
    ]
  }
  